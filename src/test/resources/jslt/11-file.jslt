import testing as t

def parts( '' ): []
def parts( elm + '\t' + rest ): [elm] + parts(rest)
def parts( rest ): [rest]

def tabbed( '' ): []
def tabbed( line + '\n' + rest ): [parts(line)] + tabbed(rest)
def tabbed( line ): [parts(line)]

t.assert("tabs", ['a'], parts("a")) +
t.assert("empty_second", ['a', ''], parts("a\t")) + 
t.assert("empty_parts", [], parts("")) + 
t.assert("three", ['a', 'b', 'c'], parts("a\tb\tc")) +
t.assert("multi", [[], ['a', ''], ['a', 'b', 'c']], tabbed("\na\t\na\tb\tc")) +
t.assert("empty", [], tabbed(""))
